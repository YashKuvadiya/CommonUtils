<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonImageInflate">
        <attr name="cil_progress_bar_color" format="color"/>
        <attr name="cil_lottie_auto_play" format="boolean"/>
        <attr name="cil_lottie_loop" format="boolean"/>
        <attr name="cil_lottie_scale_type" format="enum">
            <enum name="matrix" value="0"/>
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="cil_image_scale_type" format="enum">
            <enum name="matrix" value="0"/>
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="cil_progress_bar_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CommonToolbar">
        <attr name="tb_headerBg" format="reference|color"/>
        <attr name="tb_backVisibility" format="boolean"/>
        <attr name="tb_backIconSrc" format="reference"/>
        <attr name="tb_backIconSrcTint" format="reference|color"/>
        <attr name="tb_titleVisibility" format="boolean"/>
        <attr name="tb_titleTextColor" format="reference|color"/>
        <attr name="tb_titleTextFont" format="reference"/>
        <attr name="tb_titleText" format="string"/>
        <attr name="tb_walletVisibility" format="boolean"/>
        <attr name="tb_walletBackgroundTint" format="reference|color"/>
        <attr name="tb_walletBackground" format="reference|color"/>
        <attr name="tb_walletCoinSrc" format="reference"/>
        <attr name="tb_pointsTextColor" format="reference|color"/>
        <attr name="tb_pointsText" format="string"/>
        <attr name="tb_pointsTextFont" format="reference"/>
        <attr name="tb_historyVisibility" format="boolean"/>
        <attr name="tb_historyIconSrc" format="reference"/>
        <attr name="tb_historyIconTint" format="reference|color"/>
    </declare-styleable>
</resources>